        "widget": "dropdown"
      }
    },
    "tff": {
      "type": "boolean",
      "default": true,
      "vapoursynth": { "name": "TFF" },
      "ui": {
        "label": "Top Field First",
        "description": "Field order: true for TFF, false for BFF"
      }
    },
    "inputType": {
      "type": "integer",
      "default": 0,
